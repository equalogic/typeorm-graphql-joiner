{
  "name": "@madsci/typeorm-graphql-joiner",
  "version": "0.0.0",
  "description": "A helper to join TypeORM relations based on fields selected in a GraphQL query.",
  "author": "Madscience Ltd",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madscience/typeorm-graphql-joiner.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .ts --ignore-path .gitignore . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "postversion": "cp package.json README.md dist/",
    "release": "npx np --no-publish --contents dist/",
    "barrels:generate": "barrelsby --config barrelsby.json"
  },
  "dependencies": {
    "typeorm": "^0.2.24",
    "graphql": "^14.6.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "12.x",
    "@typescript-eslint/eslint-plugin": "=2.21.0",
    "@typescript-eslint/parser": "^2.22.0",
    "barrelsby": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "graphql-tools": "^4.0.7",
    "jest": "^25.1.0",
    "prettier": "^2.0.2",
    "sqlite3": "^4.1.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "main": "index.js",
  "np": {
    "contents": "dist",
    "publish": false,
    "yarn": false,
    "cleanup": true
  }
}
